<!--pages/home/childCpns/y-recommend/y-recommend.wxml-->
<view class="recommend">
	<block wx:for="{{recommend}}" wx:key="index">
		<view class="item">
			<image class="img" src="{{item.image}}" mode="widthFix" bindload="imgLoad" />
			<view class="text">{{item.title}}</view>
		</view>
	</block>
</view>