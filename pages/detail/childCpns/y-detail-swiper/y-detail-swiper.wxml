<!--pages/detail/childCpns/y-detail-swiper/y-detail-swiper.wxml-->
<swiper class="swiper"
        autoplay
        circular
        indicator-dots
        indicator-active-color="#fff"
        interval="3000"
        duration="300">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="item">
      <image class="img" src="{{item}}" mode="widthFix"></image>
    </swiper-item>
  </block>
</swiper>
